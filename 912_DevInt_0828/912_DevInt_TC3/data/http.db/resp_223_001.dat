#dialogBoxTitle
{
	text-overflow:ellipsis; 
	white-space:nowrap; 
	color:#636363; 
	font-size:12px; 
	overflow:hidden; 
	cursor:move; 
	font-weight:bold; 
	padding-top:4px;
}


.titleContainer
{
	border:0;cellSpacing:0;
	cellPadding:0;
	table-layout: fixed; 
	height: 24px; 

}
#closeContainer
{
	text-align:right;
}

